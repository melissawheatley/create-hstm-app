{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": 0,
    "no-eval": 2,
    "arrow-parens": 0,
    "import/first": 2,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "object-curly-newline": 0,
    "import/extensions": 0,
    "react/no-danger": 0,
    "react/jsx-curly-newline": 0
  }
}
